<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.finalprojecttry1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Word Combination"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".secondQ" />
        <activity android:name=".Fail" />
        <activity android:name=".thirdQ" />
        <activity android:name=".fourthQ" />
        <activity android:name=".finalQ" />
        <activity android:name=".win" />
        <activity android:name=".firstQ"></activity>
    </application>

</manifest>